<odoo>
    <record model="ir.ui.view" id="res_partner_company_type_form_view">
        <field name="name">
            res.partner.company.type.form (in partner_company_type_elf)
        </field>
        <field name="model">res.partner.company.type</field>
        <field
            name="inherit_id"
            ref="partner_company_type.res_partner_company_type_form_view"
        />
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="code" />
            </field>
            <field name="shortcut" position="after">
                <field name="country_id" />
                <field name="state_id" />
            </field>
        </field>
    </record>
    <record model="ir.ui.view" id="res_partner_company_type_tree_view">
        <field name="name">
            res.partner.company.type.tree (in partner_company_type_elf)
        </field>
        <field name="model">res.partner.company.type</field>
        <field
            name="inherit_id"
            ref="partner_company_type.res_partner_company_type_tree_view"
        />
        <field name="arch" type="xml">
            <field name="name" position="before">
                <field name="code" />
            </field>
            <field name="shortcut" position="after">
                <field name="country_id" />
                <field name="state_id" />
            </field>
        </field>
    </record>
</odoo>
